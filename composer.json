{
  "name": "sucheela/chinese-calendar",
  "description": "Chinese calendar utilities: heavenly stems and earthly branches calculations",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "config": {
    "lock": false
},
  "autoload": {
    "psr-4": {
      "Sucheela\\ChineseCalendar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sucheela\\ChineseCalendar\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "php": ">=7.4"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
